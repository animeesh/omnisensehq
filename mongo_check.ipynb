    "    # List all databases\n",
    "    print(\"\\nDatabases:\")\n",
    "    for db_name in client.list_database_names():\n",
    "        print(f\"- {db_name}\")\n",
    "        \n",
    "        # For each database, list its collections\n",
    "        db = client[db_name]\n",
    "        print(\"  Collections:\")\n",
    "        for coll_name in db.list_collection_names():\n",
    "            print(f\"   - {coll_name}\")\n",
    "except Exception as e:\n",
